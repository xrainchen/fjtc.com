@{
    Layout = null;
}

@{
    Layout = null;
}
@model Fjtc.Model.Entity.ProductUser
<div class="pageContent">
    <form method="post" action="@Url.Action("UpdateSendRedPackPassword")" class="pageForm required-validate" onsubmit="return validateCallback(this, dialogAjaxDone);">
        <div class="pageFormContent" layoutH="56">
            <p style="width: 500px;">
                <label style="width: 250px;">原始发送红包密码（默认登录密码）：</label>
                <input type="password" name="SourceSendRedPackPassword"/>
            </p>
            <p style="width: 500px;">
                <label style="width: 250px;">设置红包支付密码：</label>
                @Html.TextBoxFor(m => m.SendRedPackPassword)
            </p>
        </div>
        <div class="formBar">
            <ul>
                <li><div class="buttonActive"><div class="buttonContent"><button type="submit">保存</button></div></div></li>
                <li>
                    <div class="button"><div class="buttonContent"><button type="button" class="close">取消</button></div></div>
                </li>
            </ul>
        </div>
    </form>
</div>

